            android:layout_marginTop="109dp"
            android:src="@drawable/pin4" />

        <ImageView
            android:id="@+id/iv_pin5"
            android:layout_width="240dp"
            android:layout_height="168dp"
            android:layout_marginLeft="28dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="109dp"
            android:src="@drawable/pin5" />

        <ImageView
            android:id="@+id/iv_pin6"
            android:layout_width="240dp"
            android:layout_height="168dp"
            android:layout_marginLeft="28dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="109dp"
            android:src="@drawable/pin6" />

        <ImageView
            android:id="@+id/iv_pin7"
            android:layout_width="240dp"
            android:layout_height="168dp"
            android:layout_marginLeft="28dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="109dp"
            android:src="@drawable/pin7" />

        <ImageView
            android:id="@+id/iv_pin8"
            android:layout_width="240dp"
            android:layout_height="168dp"
            android:layout_marginLeft="28dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="109dp"
            android:src="@drawable/pin8" />

        <ImageView
            android:id="@+id/iv_pin9"
            android:layout_width="240dp"
            android:layout_height="168dp"
            android:layout_marginLeft="28dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="109dp"
            android:src="@drawable/pin9" />
    </FrameLayout>
